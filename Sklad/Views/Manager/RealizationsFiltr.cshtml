                    {
                    <span>Была</span>
                }
                else if (s.Shipment == false)
                {
                    <span>Не была</span>
                }
            </td>
            <td>
                @if (s.Dealer != null)
                    {
                    <span>@s.Dealer.Name</span>
                }
                else
                {
                    <span>@s.Number</span>
                }
            </td>
            <td>@s.Comment</td>
        </tr>
    }
    else
    {
        <tr>
            <td>@s.Date</td>
