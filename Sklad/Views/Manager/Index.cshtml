
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutManager.cshtml";
}

<div class="row">
    <div class="col-md-12" align="center"><h3>Список розниц</h3></div>
</div>
@foreach(var s in Model)
{
    <div class="row">
        <div class="col-md-6" align="center"><h4>@s.Name</h4></div>
        <div class="col-md-6" align="center"><a href="/Manager/Stock/@s.Id">Перейти</a></div>
    </div>
}

