
@{
    ViewBag.Title = "StockInfoPacks";
    Layout = "~/Views/Shared/_LayoutManager.cshtml";
}

<div class="head">
    <h4>Список Пакетов</h4>
</div>
<table class="table table-bordered">
    <thead style="background-color: white; opacity: 1;">
        <tr>
            <td>Название</td>
            <td>Группа</td>
            <td>Кол-во</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var p in Model)
        {
            <tr>
                <td>@p.Name</td>
                <td>@p.Group</td>
                <td>@p.Amount</td>
            </tr>
        }
    </tbody>
</table>
